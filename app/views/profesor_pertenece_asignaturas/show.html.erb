<div class="container">
  <div id="header">
    <img src="/assets/ull_color_horizontal2.jpg" class="img-rounded" alt="Asignatura" width=50% height= 50% />
    <div class="page-header">
      <h1>Profesor de la asignatura <small><%= @profesor_pertenece_asignatura.id %></small></h1>
    </div>
  </div>

<p id="notice"><%= notice %></p>

<p>
  <b>Asignatura:</b>
  <!--<%= @asig = Asignatura.find(@profesor_pertenece_asignatura.asignatura_id)%>-->
  <%= @asig.nombre %>
  <%#= @profesor_pertenece_asignatura.asignatura_id%>
</p>

<p>
  <b>Profesor:</b>
  <!--<%= @profe = Profesore.find(@profesor_pertenece_asignatura.profesore_id)%>-->
  <%= @profe.name %>
  <%#= @profesor_pertenece_asignatura.profesore_id %>
</p>

<p>
  <b>Rol:</b>
  <%= @profesor_pertenece_asignatura.rol %>
</p>

  <div class="btn">
    <%= link_to 'Editar', edit_profesor_pertenece_asignatura_path(@profesor_pertenece_asignatura) %>
  </div>
  <div class="btn">
    <%= link_to 'Volver', profesor_pertenece_asignaturas_path %>
  </div>
</div>
</div>