    <td><%= competency_pertenece_prueba.id %></td>
    <td><%= competency_pertenece_prueba.asignatura_id %></td>
    <td><%= competency_pertenece_prueba.competency_id %></td>
    <td><%= competency_pertenece_prueba.prueba_id %></td>
